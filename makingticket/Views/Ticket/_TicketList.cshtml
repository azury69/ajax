@model IEnumerable<makingticket.Models.Ticket>

<h3>Ticket List</h3>
<ul id="ticketList">
    @foreach (var ticket in Model)
    {
        <li>
            <strong>@ticket.Name</strong> - @ticket.Description
            <button class="view-details btn btn-info" data-id="@ticket.Id">View Details</button>
        </li>
    }
</ul>
